# WhatsApp Server Environment Variables
# Copy this file to .env and fill in your actual values

# AssemblyAI API Key for voice transcription
# Get your API key from: https://www.assemblyai.com/
ASSEMBLYAI_API_KEY=your_assemblyai_api_key_here

# Voice Bridge URL for AI processing
# This is the URL where your AI service is running
VOICE_BRIDGE_URL=http://localhost:3001

# Server Configuration
# Port for the WhatsApp server (optional, defaults to 3000)
PORT=3000

# WhatsApp Configuration
# These are automatically managed by Baileys, but you can override if needed
# WHATSAPP_SESSION_PATH=./baileys_auth_info

# Optional: Logging level
# LOG_LEVEL=info

# Optional: Enable debug mode
# DEBUG=true
